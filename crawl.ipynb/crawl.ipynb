   ],
   "source": [
    "f = open(\"red_velvet.txt\", \"w\")\n",
    "\n",
    "for i in red_list :\n",
    "    temphtml = requests.get(base_url + i).text\n",
    "    tempsoup = BeautifulSoup(temphtml, 'html.parser')\n",
    "    try :\n",
    "        temp = str(tempsoup.select('#lyricText')[0])\n",
    "    except :\n",
    "        print(i)\n",
    "    temp = temp[40:-6].replace(\"<br/>\", \"\\n\")\n",
    "    f.write(temp)\n",
    "    \n",
    "f.close()\n",
    "# 87 - 3 = 84"
   ]
